{
  "manifest_version": 2,
  "name": "Dazn Media Player",
  "version": "1.0",
  "description": "Manipulates the media player to get a wider screen.",
  "icons": {
    "48": "icons/icon_48.png",
    "96": "icons/icon_96.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}